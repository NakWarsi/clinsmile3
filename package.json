{
  "name": "dentriz-web",
  "version": "0.0.0",
  "scripts": {
    "start": "node server.js",
    "build": "ng build --configuration production --base-href / && node scripts/copy-404.js",
    "build:static": "ng build --configuration production --base-href / --output-path=dist/DentrizWeb/browser && node scripts/copy-404.js",
    "build:github-pages": "ng build --configuration production --base-href / --output-path=dist/DentrizWeb/browser && node scripts/copy-404.js",
    "build:github-pages-subdir": "ng build --configuration production --base-href /clinsmile3/ --output-path=dist/DentrizWeb/browser && node scripts/copy-404.js",
    "postinstall": "npm run build:static"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.5",
    "express": "^4.21.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.5",
    "@angular/cli": "^20.1.5",
    "@angular/compiler-cli": "^20.1.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
